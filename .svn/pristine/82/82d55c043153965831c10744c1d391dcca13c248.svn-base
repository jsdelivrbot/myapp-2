import React from 'react';

const WpForumItem = (props) => (
	<li className='wpForumItem border1px'>
		<a href={ props.link } title='' className='wpHead'>
			<img src={ props.img } alt='' />
		</a>
		<div className='wpForumCentent'>
			<p className='wpName'>{ props.name }</p>
			<p className='wpForumTitle'>{ props.title }</p>
			<p className='wpForumText'>{ props.text }</p>
			{
				props.wpForumFeatures
			}
			{
				props.children
			}
		</div>
	</li>
)

export default WpForumItem


